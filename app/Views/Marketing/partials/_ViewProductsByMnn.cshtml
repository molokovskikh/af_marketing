@using Marketing.ViewModels
@model IList<MnnCatalogViewModel>

<table class="table table-striped table-condensed">
		<thead>
				<tr>
						<th>Наименование</th>
						<th>Форма выпуска</th>
				</tr>
		</thead>
		<tbody>
				@foreach (var item in Model)
        {
				<tr>
						<td>@item.CatalogName</td>
						<td>@item.CatalogForm</td>
				</tr>
        }
		</tbody>
</table>
